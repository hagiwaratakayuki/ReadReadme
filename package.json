{
  "name": "readreadme",
  "displayName": "ReadReadme",
  "description": "Open Homepage ",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "HagiwaraTakayuki",
  "engines": {
    "vscode": "^1.69.0"
  },
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/hagiwaratakayuki/ReadReadme.git"
  },
  "bugs": {
    "url": "https://github.com/hagiwaratakayuki/ReadReadme/issues"   
  },
  "author":{
    "name":"Hagiwara Takayuki",
    "email": "hagiwaratakyuki@gmail.com"

  },
  "categories": [
   "Programming Languages",
   "Other"
  ],
  "keywords" : [
    "document",
    "redame",
    "homepage"
  ],
  "activationEvents": [
    "onCommand:readreadme.readreadme"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "readreadme.readreadme",
        "title": "%readreadme_title%"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "readreadme.readreadme",
          "group": "ReadReadMe@1"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.69.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "just-safe-get": "^4.0.1",
    "node-fetch-cjs": "^3.2.9"
  }
}
